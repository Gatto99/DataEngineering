-------------------------------INTER SITE-------------------------------
[XPATH]
a. Nome e Cognome = $x("//div[contains(@class,'css-1riajt')]/div[contains(@class,'css-79elbk')]/h1[contains(@class,'css-oq49di')]/span/text()")
b. Eta = $x("//p[text()='Età']/preceding-sibling::p/text()")
c. Altezza = $x("(//p[text()='Altezza']/preceding-sibling::p/text())[1]")
d. Presenze = $x("//p[text()='Partite giocate']/following-sibling::p/span/text()")
e. Gol = $x("//p[text()='Gol segnati']/following-sibling::p/span/text()")
f. Ammonizioni = $x("//p[text()='Cartellini gialli']/following-sibling::p/span/text()")
g. Espulsioni = $x("//p[text()='Cartellini rossi']/following-sibling::p/span/text()")
h. Contrasti vinti = $x("//p[text()='Contrasti vinti']/following-sibling::p/span/text()")
i. Disimpegni = $x("//p[text()='Disimpegni']/following-sibling::p/span/text()")

