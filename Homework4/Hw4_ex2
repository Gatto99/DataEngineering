-----------------------------SPORT VIRGILIO-----------------------------
[XPATH]
a. Nome e Cognome = $x("//h1[contains(@class, 'title-page')]/text()")
b. Età = $x("//b[text()='Età:']/following-sibling::text()")
c. Nazionale = $x("//b[text()='Nazionale:']/following-sibling::text()")
d. Nazionalità = $x("//b[text()='Nazionalità:']/following-sibling::text()")
e. Piede = $x("//b[text()='Piede preferito:']/following-sibling::text()")
f. Peso = $x("//b[text()='Peso:']/following-sibling::text()")
g. Numero maglia = $x("(//span[text()='Numero di maglia']/preceding-sibling::span/text())[1]")
h. Minuti giocati = $x("(//span[text()='Minuti giocati']/preceding-sibling::span/text())[1]")
i. Gol = $x("(//span[text()='Gol fatti']/preceding-sibling::span/text())[1]")
l. Assist = $x("(//span[text()='Assist']/preceding-sibling::span/text())[1]")

