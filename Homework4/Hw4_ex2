-----------------------------TUTTOCALCIATORI----------------------------
[XPATH]
a. Nome e Cognome = $x("//h1/text()")
b. Età = $x("//td[text()='Età']/following-sibling::td/text()")
c. Ruolo = $x("(//strong[text()='RUOLO']/following-sibling::text())[1]")
d. Piede = $x("(//strong[text()='PIEDE CALCIO']/following-sibling::text())[1]")
e. Scadenza contratto = $x("(//strong[text()='SCADENZA CONTRATTO']/following-sibling::text())[1]")
f. Procuratore = $x("(//strong[text()='PROCURATORE']/following-sibling::text())[1]")
g. Esordio in nazionale = $x("(//strong[text()='ESORDIO IN NAZIONALE']/following-sibling::text())[1]")
h. Presenze in nazionale = $x("(//strong[text()='PRESENZE IN NAZIONALE']/following-sibling::text())[1]")
i. Presenze nei campionati = $x("//tr[contains(@class,'tr_campionato totale')]/td[4]/text()")
l. Gol 2022-2023 =  $x("//tr[contains(@class,'tr_campionato')]/td[text()='2022-2023']/following-sibling::td[4]/strong/text()")

